/* Solution written in JavaScript for Two Sum II on LeetCode 
 * @param {number[]} numbers
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(numbers, target) {
    var min = 0;
    var max = numbers.length-1;
    var fin = new Array(2);
    while (numbers[min] + numbers[max] != target) {
        if (numbers[min] + numbers[max] < target) {
            min++;
        } else {
            max--;
        }
    }
    fin[0] = min+1;
    fin[1] = max+1;
    return fin;

};
